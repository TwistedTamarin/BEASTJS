{
  "name": "tamarin-beast",
  "version": "0.1.0",
  "description": "TwistedTamarin's BEAST, a small library for websites",
  "license": "MIT",
  "scripts": {
    "serve": "rollup -c rollup.config.js --watch",
    "build": "rollup -c rollup.prod.config.js",
    "less:dev": "lessc --glob less/index.less assets/style.min.css",
    "less:prod": "lessc --glob --clean-css less/index.less assets/style.min.css"
  },
  "private": true,
  "keywords": [
    "BEAST",
    "TwistedTamarin"
  ],
  "homepage": "https://beast.twistedtamarin.com",
  "devDependencies": {
    "rollup": "^0.58.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-server": "^0.7.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-glob": "^2.0.2"
  }
}
